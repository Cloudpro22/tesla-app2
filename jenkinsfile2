pipeline{
  agent { label 'ansible' }
  
  
 stages {
    stage('1checkmemory'){
      steps{
        sh "ansible localhost -m shell -a 'free -m' "
       
      }
    }
    
    stage('2checknode'){
      steps{
        sh "ansible kops -m shell -a 'kubectl get node' "
       
      }
    }
    
    }
    }
